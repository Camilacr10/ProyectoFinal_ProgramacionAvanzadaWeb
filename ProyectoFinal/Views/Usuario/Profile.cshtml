@model ProyectoFinalBLL.DTOs.UsuarioDto

@{
    ViewData["Title"] = "Mi Perfil";
}

<div class="page-header">
    <h2 class="m-0">@ViewData["Title"]</h2>
</div>

<div class="card p-3 rounded-2xl shadow-soft">
    <p class="mb-1"><strong>Nombre:</strong> @Model.NombreCompleto</p>
    <p class="mb-1"><strong>Correo:</strong> @Model.Correo</p>
    <p class="mb-0"><strong>Rol:</strong> @Model.Rol</p>
</div>
